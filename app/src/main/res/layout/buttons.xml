<?xml version="1.0" encoding="utf-8"?>

<merge xmlns:android="http://schemas.android.com/apk/res/android">
        <Button
            android:id="@+id/button_all_clear"
            android:text="AC"
            android:onClick="pushAllClear"/>

        <Button
            android:id="@+id/button_positive_negative"
            android:text="+/-"
            android:onClick="pushPlusMinus"/>

        <Button
            android:id="@+id/button_percent"
            android:text="%"
            android:onClick="pushPercent"/>

        <Button
            android:id="@+id/button_period"
            android:text="."
            android:onClick="pushDecimal"/>

        <Button
            android:id="@+id/button_1"
            android:onClick="pushOne"
            android:text="1" />

        <Button
            android:id="@+id/button_2"
            android:text="2"
            android:onClick="pushTwo"/>

        <Button
            android:id="@+id/button_3"
            android:text="3"
            android:onClick="pushThree"/>

        <Button
            android:id="@+id/button_divide"
            android:text="/"
            android:onClick="pushDivde"/>

        <Button
            android:id="@+id/button_4"
            android:text="4"
            android:onClick="pushFour"/>

        <Button
            android:id="@+id/button_5"
            android:text="5"
            android:onClick="pushFive"/>

        <Button
            android:id="@+id/button_6"
            android:text="6"
            android:onClick="pushSix"/>

        <Button
            android:id="@+id/button_multiply"
            android:text="*"
            android:onClick="pushMultiply"/>

        <Button
            android:id="@+id/button_7"
            android:text="7"
            android:onClick="pushSeven"/>

        <Button
            android:id="@+id/button_8"
            android:text="8"
            android:onClick="pushEight"/>

        <Button
            android:id="@+id/button_9"
            android:text="9"
            android:onClick="pushNine"/>

        <Button
            android:id="@+id/button_subtract"
            android:text="-"
            android:onClick="pushSubtract"/>
        <Button
            android:id="@+id/button_clear"
            android:text="@string/clear"
            android:onClick="pushClear"/>
        <Button
            android:id="@+id/button_0"
            android:text="0"
            android:layout_rowSpan="2"
            android:onClick="pushZero"/>

        <Button
            android:id="@+id/button_equals"
            style="@style/ButtonStyle"
            android:text="="
            android:onClick="pushEquals"/>


        <Button
            android:id="@+id/button_add"
            android:text="+"
            android:onClick="pushAdd"/>


</merge>